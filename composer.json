{
    "name": "skaisser/laramail",
    "type": "library",
    "description": "Centralize all user related emails and keep track of everything!",
    "keywords": [
        "skaisser",
        "laramail",
        "laravel",
        "php",
        "email"
    ],
    "homepage": "https://github.com/skaisser/laramail",
    "license": "MIT",
    "authors": [
        {
            "name": "Shirleyson Kaisser",
            "email": "skaisser@gmail.com",
            "homepage": "http://www.skaisser.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "5.5+"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Laramail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramail\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
